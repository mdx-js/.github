{
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "bugs": "https://github.com/mdx-js/.github",
  "contributors": [
    "Christian Murphy (https://github.com/ChristianMurphy)",
    "John Otander <johnotander@gmail.com> (http://johnotander.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "description": "Community health files",
  "devDependencies": {
    "prettier": "^3.0.0",
    "remark-cli": "^12.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-preset-wooorm": "^11.0.0",
    "xo": "^0.59.0"
  },
  "keywords": [],
  "license": "CC-BY-4.0",
  "name": "github",
  "prettier": {
    "bracketSpacing": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "remark-frontmatter",
      "remark-preset-wooorm",
      [
        "remark-lint-first-heading-level",
        false
      ]
    ]
  },
  "repository": "mdx-js/.github",
  "scripts": {
    "format": "remark --frail --quiet --output -- . && prettier --log-level warn --write -- .",
    "test": "npm run format"
  },
  "version": "0.0.0",
  "xo": {
    "overrides": [
      {
        "files": [
          "sandbox-templates/mdx-evaulate-node/index.js"
        ],
        "rules": {
          "new-cap": "off"
        }
      }
    ],
    "prettier": true
  },
  "workspaces": [
    "sandbox-templates/*/"
  ]
}
